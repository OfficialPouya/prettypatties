<?xml version="1.0"?>
<robot name="block">

<link name="cheese_link">

  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <!-- <mesh filename="package://ur_description/meshes/burger_v1.stl" scale="0.003 0.003 0.003"/> -->
      <box size="0.15 0.15 0.015" />
    </geometry>
  </collision>

  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://ur_description/meshes/cheese_v3.stl" scale="0.003 0.003 0.015"/>
    </geometry>
  </visual>

  <inertial>
   <origin xyz="0 0 0" rpy="0 0 0"/>
   <mass value="0.0225" />
   <inertia
      ixx="0.0" ixy="0.0"  ixz="0.0"
      iyy="0.0" iyz="0.0"  izz="0.0" />
  </inertial>
  <static>true</static>
</link>

<gazebo reference="cheese_link">
  <static>1</static>
  <material>Gazebo/Yellow</material>
  <mu1>100</mu1>
  <mu2>100</mu2>
  <kp>2147483647</kp>
  <kd>1</kd>
  <maxVel>0</maxVel>
  <minDepth>0.001</minDepth>
</gazebo>

</robot>